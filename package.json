{
	"name": "biubiu.tools",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "bun run build && wrangler dev",
		"deploy": "bun run build && wrangler deploy",
		"cf-typegen": "wrangler types types/worker-configuration.d.ts",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "bun run test:unit -- --run && bun run test:e2e",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"wrangler": "^4.28.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-cloudflare": "^7.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@vitest/browser": "^3.2.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"mdsvex": "^0.12.3",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"dependencies": {
		"@codemirror/autocomplete": "^6.18.7",
		"@codemirror/commands": "^6.8.1",
		"@codemirror/language": "^6.11.3",
		"@codemirror/state": "^6.5.2",
		"@codemirror/theme-one-dark": "^6.1.3",
		"@codemirror/view": "^6.38.2",
		"@lucide/svelte": "^0.552.0",
		"@shelchin/ethereum-connectors": "0.0.1-dev.20251106040039.5fa58a7",
		"@shelchin/i18n": "0.0.3-dev.20251016115159.4e313f3",
		"class-variance-authority": "^0.7.1",
		"codemirror": "^6.0.2",
		"idb-keyval": "^6.2.2",
		"lucide-svelte": "^0.552.0",
		"qr-code-styling": "^1.9.2",
		"viem": "2.38.4"
	}
}
